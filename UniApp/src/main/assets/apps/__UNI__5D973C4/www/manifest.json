{
  "@platforms": [
    "android",
    "iPhone",
    "iPad"
  ],
  "id": "__UNI__5D973C4",
  "name": "warehouseApp",
  "version": {
    "name": "1.0.0",
    "code": "100"
  },
  "description": "",
  "launch_path": "__uniappview.html",
  "developer": {
    "name": "",
    "email": "",
    "url": ""
  },
  "permissions": {
    "UniNView": {
      "description": "UniNView原生渲染"
    }
  },
  "plus": {
    "useragent": {
      "value": "uni-app",
      "concatenate": true
    },
    "splashscreen": {
      "target": "id:1",
      "autoclose": true,
      "waiting": true,
      "delay": 0
    },
    "popGesture": "close",
    "launchwebview": {
      "id": "1",
      "kernel": "WKWebview"
    },
    "statusbar": {
      "immersed": "supportedDevice",
      "style": "light",
      "background": "#409EFF"
    },
    "usingComponents": true,
    "nvueCompiler": "uni-app",
    "compilerVersion": 3,
    "allowsInlineMediaPlayback": true,
    "safearea": {
      "background": "#ffffff",
      "bottom": {
        "offset": "auto"
      }
    },
    "uni-app": {
      "compilerVersion": "2.8.2",
      "control": "uni-v3",
      "nvueCompiler": "uni-app",
      "renderer": "auto",
      "nvue": {
        "flex-direction": "column"
      },
      "nvueLaunchMode": "normal"
    },
    "tabBar": {
      "color": "#7A7E83",
      "selectedColor": "#409EFF",
      "borderStyle": "rgba(0,0,0,0.4)",
      "backgroundColor": "#ffffff",
      "height": "55px",
      "fontSize": "10px",
      "iconWidth": "24px",
      "spacing": "6px",
      "list": [
        {
          "pagePath": "pages/Home/index",
          "iconPath": "static/home.png",
          "selectedIconPath": "static/home_sel.png",
          "text": "首页"
        },
        {
          "pagePath": "pages/Statistical/index",
          "iconPath": "static/tongji.png",
          "selectedIconPath": "static/tongji_sel.png",
          "text": "统计"
        },
        {
          "pagePath": "pages/Mine/index",
          "iconPath": "static/mine.png",
          "selectedIconPath": "static/mine_sel.png",
          "text": "我的"
        }
      ]
    },
    "launch_path": "__uniappview.html"
  }
}